<script type="text/javascript">
var at = [
	'right bottom'
],
my = [
	'left bottom'
],
styles = [
	 'dark'
];
 
// Create the tooltips only on document load
$(document).ready(function()
{
// Loop through the my array
for(var i = 0; i < my.length; i++)
{
$('.structure')

	.removeData('qtip') 
	.qtip({
		content: {
			text: 'At its ' + at[i], 
			title: {
				text: 'My ' + my[i],
				button: true
			}
		},
		position: {
			my: my[i], 
			at: at[i] 
		},
		show: {
			event: false, 
			ready: true
		},
		hide: false, 
		style: {
			classes: 'ui-tooltip-shadow ui-tooltip-' + styles[i]
		}
	});
}
});

</script>


{rodape}
